{
  "extends": [
    "eslint-config-airbnb-base",
    "plugin:jest/recommended",
    "../../.eslintrc.notice.json"
  ],
  "rules": {
    "import/extensions": 0,
    "import/no-extraneous-dependencies": 0,
    "linebreak-style": 0,
    "import/prefer-default-export": 0,
    "no-underscore-dangle": 0
  },
  "env": {
    "browser": true,
    "jest/globals": true
  },
  "plugins": ["jest"],
  "settings": {
    "import/resolver": "webpack"
  },
}
